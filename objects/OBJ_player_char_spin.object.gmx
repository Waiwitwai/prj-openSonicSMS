        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Moves

// --&gt;
if (image_xscale == 1) 
{   
    // Moves
    hspeed -= global.valSpeed;
    if (hspeed &lt;= 0) 
    {
        hspeed = 0;
    }
    
    // Hold
    if (global.btLeftPress)
    {
        hspeed = hspeed/5;
    }
}

// &lt;--
if (image_xscale == -1) 
{
    // Moves
    hspeed += global.valSpeed;
    if (hspeed &gt;= 0) 
    {
        hspeed = 0;
    }
    
    // Hold
    if (global.btRightPress)
    {
        hspeed = hspeed/5;
    }
}

// Back to normal

if (hspeed == 0 &amp;&amp; !place_free(x, y+2))
{
    instance_change(OBJ_player_char,true);
}
