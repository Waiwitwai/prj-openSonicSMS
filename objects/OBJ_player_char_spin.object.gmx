            <string>/// Margin

// View
if (x-9+hspeed &lt; view_xview[0] || x+6+hspeed &gt; view_xview[0]+view_wview[0])
{   
    if (gravity == 0) 
    {
        sprite_index = SPR_player_stop;
    }
    hspeed = 0;
}

// Room
if (x+9+hspeed &lt;= 9 || x+hspeed &gt;= room_width-9)
{
    if (gravity == 0) 
    {
        sprite_index = SPR_player_stop;
    }
    hspeed = 0;
}
