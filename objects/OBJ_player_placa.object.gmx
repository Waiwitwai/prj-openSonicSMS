//Chão
if (!place_free(x+hspeed, y+vspeed) &amp;&amp; !place_meeting(x+hspeed, y, OBJ_mask_parede))
{
    vspeed = 0;
    global.sonicPulo = false;
    global.sonicPuloMola = false;
    playerFly = false;
    timeFly = true;
}
//Chão + Parede
if (!place_free(x, y+vspeed) &amp;&amp; place_meeting(x+hspeed, y, OBJ_mask_parede))
{
    vspeed = 0;
    global.sonicPulo = false;
    global.sonicPuloMola = false;
    playerFly = false;
    timeFly = true;
}

//Paredes
if (place_meeting(x+hspeed, y, OBJ_mask_parede) &amp;&amp; !place_meeting(x+hspeed, y, OBJ_mask_teto))
{
    if (speed &gt; 0) {hspeed -= hspeed;}
    sonicImaParado = 0;
    okParado = false;
}

//Teto
if (place_meeting(x, y+vspeed, OBJ_mask_teto) &amp;&amp; !place_meeting(x+hspeed, y, OBJ_mask_parede))
{
    if (speed &gt; 0) {vspeed -= vspeed/2;}
}
//Teto com função de parede
if (place_meeting(x+hspeed, y, OBJ_mask_teto))
{
    if (speed &gt; 0) {hspeed -= hspeed;}
    gravity = global.valGravity*1.5;
    global.sonicPulo = true;
    sonicImaParado = 0;
    okParado = false;
}
